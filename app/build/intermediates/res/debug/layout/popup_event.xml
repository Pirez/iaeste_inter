<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        android:id="@+id/popup_event"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        xmlns:android="http://schemas.android.com/apk/res/android"

        >

    <TextView
            android:id="@+id/text_popup_event_overskrift"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Overksrift"
            android:textSize="35dp"
            android:textStyle="bold"
            android:layout_marginBottom="25sp"
            android:paddingTop="0dp"
            android:layout_marginTop="0dp"
            android:layout_centerHorizontal="true"

        >
    </TextView>
    <TextView
            android:id="@+id/text_popup_event_sted"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/kalender_01"
            android:textSize="25sp"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/text_popup_event_overskrift"
            android:layout_marginBottom="5sp"

        >
    </TextView>
    <TextView
            android:id="@+id/text_popup_event_dato"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/kalender_02"
            android:textSize="20sp"
            android:layout_below="@+id/text_popup_event_sted"
            android:layout_marginBottom="5sp"
             android:layout_centerHorizontal="true"


        >
    </TextView>
    <TextView
            android:id="@+id/text_popup_evet_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text="INFORMASJON"
            android:layout_below="@+id/text_popup_event_dato"
          android:layout_centerHorizontal="true"

        >
    </TextView>
    <Button
            android:id="@+id/button_popup_event_lagre"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:padding="5dp"
            android:text="@string/kalender_03"
            android:layout_below="@+id/text_popup_evet_info"
            android:visibility="gone"
            android:layout_alignParentLeft="false"
            android:layout_centerHorizontal="true"
            >
    </Button>
    <Button
            android:id="@+id/button_popup_event_tilbake"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:text="@string/kalender_04"
            android:layout_below="@+id/button_popup_event_lagre"
            android:layout_alignParentLeft="false"
            android:layout_centerHorizontal="true"
            >
    </Button>
</RelativeLayout>
